{
  "name": "eslint-plugin-prisma",
  "version": "0.0.0",
  "description": "ESLint plugin ensuring best practices and code quality for Prisma with TypeScript",
  "keywords": [
    "Prisma",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Lucas GrÃ©goire",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "mocha tests --recursive",
    "compile": "tsc --project tsconfig.json --no-emit",
    "build": "tsc --project tsconfig.build.json",
    "lint": "npm-run-all \"lint:*\"",
    "lint:docs": "markdownlint \"**/*.md\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:ts": "eslint .",
    "update:eslint-docs": "npm run build && eslint-doc-generator"
  },
  "dependencies": {
    "@typescript-eslint/types": "^7.13.0",
    "@typescript-eslint/utils": "^7.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-doc-generator": "^1.7.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-plugin": "^6.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "markdownlint-cli": "^0.41.0",
    "mocha": "^10.4.0",
    "npm-run-all2": "^6.2.0"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "license": "MIT",
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  }
}
